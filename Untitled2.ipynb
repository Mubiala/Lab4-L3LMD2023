   "source": [
    "import hashlib\n",
    "#demander à l'utilisateur d'entrer sa chaine à hacher\n",
    "data=input(\"entrer la chaine à hacher:\")\n",
    "\n",
    "#création de l'objet de hachage\n",
    "hash_object=hashlib.sha256(data.encode())\n",
    "\n",
    "#calcul du hachage\n",
    "hash_hex=hash_object.hexdigest()\n",
    "\n",
    "#affichage\n",
    "print(hash_hex)"
   ]
